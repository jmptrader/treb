{
  "name": "treb",
  "version": "12.0.3",
  "main": "dist/index.js",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.13.34",
    "@microsoft/api-extractor": "^7.18.4",
    "@types/base64-js": "^1.3.0",
    "@types/file-saver": "^2.0.2",
    "@types/he": "^1.1.1",
    "@types/js-base64": "^3.3.1",
    "@types/node": "^16.4.12",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "archiver": "^5.3.0",
    "base64-js": "^1.3.1",
    "buffer": "^6.0.3",
    "core-js": "^3.13.0",
    "css-loader": "^6.2.0",
    "eslint": "^7.27.0",
    "fast-xml-parser": "^3.19.0",
    "file-saver": "^2.0.5",
    "fs-extra": "^10.0.0",
    "he": "^1.2.0",
    "jszip": "^3.6.0",
    "license-checker-webpack-plugin": "^0.2.1",
    "loader-utils": "^2.0.0",
    "mini-css-extract-plugin": "^2.2.0",
    "riskampjs-mc": "git+ssh://git@gitlab.com/SDLLC/riskampjs-mc.git",
    "sass": "^1.34.0",
    "sass-loader": "^12.1.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.2",
    "ts-node-dev": "^1.1.8",
    "tslib": "^2.2.0",
    "typescript": "^4.4.0-beta",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "z85-codec": "^0.1.0"
  },
  "scripts": {
    "build": "node compile2.js",
    "dev": "node compile2.js -d --modern",
    "dev-legacy": "node compile2.js -d --legacy",
    "watch": "node compile2.js -w",
    "generate-api": "ts-node-dev api-types/api2.ts --config api-config.json",
    "watch-modern": "node compile2.js -w --modern -d",
    "watch-legacy": "node compile2.js -w --legacy -d",
    "build-modern": "node compile2.js --modern",
    "build-legacy": "node compile2.js --legacy",
    "update-icons": "node treb-embed/script/process-icons.js --filter unused --source treb-embed/icons/4 > treb-embed/src/generated/toolbar42.ts",
    "watch-dev": "node compile2.js -w -d",
    "release": "node compile2.js --clean && node zip-package.js"
  },
  "build-entry-points": {
    "main": "embedded-treb-bundle",
    "calculation-worker": "embedded-treb-worker",
    "export-worker": "embedded-treb-export-worker",
    "engine": "treb-engine",
    "connect": "treb-connect-lib"
  }
}
