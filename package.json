{
  "name": "treb",
  "version": "16.4.0",
  "main": "dist/index.js",
  "devDependencies": {
    "@types/base64-js": "^1.3.0",
    "@types/file-saver": "^2.0.2",
    "@types/he": "^1.1.1",
    "@types/js-base64": "^3.3.1",
    "@types/node": "^17.0.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "archiver": "^5.3.0",
    "base64-js": "^1.3.1",
    "buffer": "^6.0.3",
    "core-js": "^3.13.0",
    "css-loader": "^6.2.0",
    "cssnano": "^5.0.15",
    "esbuild": "^0.14.11",
    "esbuild-sass-plugin": "^2.0.3",
    "eslint": "^8.5.0",
    "fast-xml-parser": "^3.19.0",
    "file-saver": "^2.0.5",
    "fs-extra": "^10.0.0",
    "he": "^1.2.0",
    "jszip": "^3.6.0",
    "license-checker-webpack-plugin": "^0.2.1",
    "loader-utils": "^3.2.0",
    "mini-css-extract-plugin": "^2.2.0",
    "postcss": "^8.4.5",
    "riskampjs-mc": "file:../riskampjs-mc",
    "sass": "^1.34.0",
    "sass-loader": "^12.1.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.2.1",
    "terser": "^5.10.0",
    "treb-base-types": "file:lib/treb-base-types",
    "treb-calculator": "file:lib/treb-calculator",
    "treb-charts": "file:lib/treb-charts",
    "treb-format": "file:lib/treb-format",
    "treb-grid": "file:lib/treb-grid",
    "treb-parser": "file:lib/treb-parser",
    "treb-utils": "file:lib/treb-utils",
    "ts-loader": "^9.2.2",
    "ts-node-dev": "^1.1.8",
    "tslib": "^2.2.0",
    "typescript": "^4.4.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "z85-codec": "^0.1.0"
  },
  "scripts": {
    "build": "node esbuild.js",
    "dev": "node esbuild.js --dev",
    "watch": "node esbuild.js --watch",
    "tsc": "tsc -b treb-embed/modern.tsconfig.json",
    "generate-api": "ts-node-dev api-types/api2.ts --config api-config.json && cp treb.d.ts release-distribution-files/",
    "watch-api": "ts-node-dev --respawn api-types/api2.ts --config api-config.json && cp treb.d.ts release-distribution-files/",
    "watch-modern": "node esbuild.js --watch --modern --dev",
    "build-modern": "node esbuild.js --modern",
    "watch-cookie": "node esbuild.js --watch --cookie --dev",
    "build-cookie": "node esbuild.js --cookie",
    "update-icons": "node treb-embed/script/process-icons.js --filter unused --source treb-embed/icons/4 > treb-embed/src/generated/toolbar42.ts",
    "release": "node esbuild.js --clean --modern --module && npm run tsc && npm run generate-api && node copy-distribution-files.js && node zip-package.js"
  },
  "build-entry-points": {
    "main": "treb-bundle",
    "calculation-worker": "treb-worker",
    "export-worker": "treb-export-worker"
  }
}
